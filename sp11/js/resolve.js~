function resolveFormulas() {
  $('.symbol').each(function() {
    if ($(this).attr('data-content')) {
      var formula = $(this).attr('data-content');
      formula = formula.replace('bha','Ë‰');
      formula = formula.replace('ha','Ë˜');
      formula = formula.replace('au','Ë‰Ë˜Ë˜Ë‰Ë‰');
      formula = formula.replace('Ã±','Ë‰Ë˜Ë˜Ë‰Ë‰');
      formula = formula.replace('ai','Ë˜Ë‰Ë˜Ë‰Ë˜Ë‰');
      formula = formula.replace('á¹­','Ë˜Ë‰Ë˜Ë‰Ë˜Ë‰');
      formula = formula.replace('o','Ë‰Ë˜Ë‰Ë‰');
      formula = formula.replace('á¹£','Ë‰Ë˜Ë‰Ë‰');
      formula = formula.replace('a','Ë˜Ë˜Ë˜');
      formula = formula.replace('m','Ë˜Ë˜Ë˜');
      formula = formula.replace('Ä','Ë‰Ë˜Ë˜');
      formula = formula.replace('t','Ë‰Ë˜Ë˜');
      formula = formula.replace('i','Ë˜Ë‰Ë˜');
      formula = formula.replace('k','Ë˜Ë‰Ë˜');
      formula = formula.replace('e','Ë˜Ë‰Ë‰');
      formula = formula.replace('b','Ë˜Ë‰Ë‰');
      formula = formula.replace('u','Ë˜Ë˜Ë‰');
      formula = formula.replace('l','Ë˜Ë˜Ë‰');
      formula = formula.replace('Ä«','Ë‰Ë˜Ë‰');
      formula = formula.replace('Å›','Ë‰Ë˜Ë‰');
      formula = formula.replace('á¹›','Ë˜Ë‰Ë‰');
      formula = formula.replace('á¹…','Ë˜Ë‰Ë‰');
      formula = formula.replace('Å«','Ë‰Ë‰Ë‰');
      formula = formula.replace('g','Ë‰Ë‰Ë‰');
      formula = formula.replace('á¹…','Ë˜Ë‰Ë‰');
      formula = formula.replace('d','Ë˜Ë˜Ë˜Ë˜');
      formula = formula.replace('r','Ë˜Ë˜');
      formula = formula.replace('p','Ë‰Ë˜');
      formula = formula.replace('j','Ë˜Ë‰');
      formula = formula.replace('s','Ë‰Ë‰');
      $(this).attr('data-content',formula);
    }
  });
  $('.bsymbol').each(function() {
    if ($(this).attr('data-content')) {
      var formula = $(this).attr('data-content');
      formula = formula.replace('bha','ğ‘€•');
      formula = formula.replace('ha','ğ‘€­');
      formula = formula.replace('au','ğ‘€•ğ‘€­ğ‘€­ğ‘€•ğ‘€•');
      formula = formula.replace('Ã±','ğ‘€•ğ‘€­ğ‘€­ğ‘€•ğ‘€•');
      formula = formula.replace('ai','ğ‘€­ğ‘€•ğ‘€­ğ‘€•ğ‘€­ğ‘€•');
      formula = formula.replace('á¹­','ğ‘€­ğ‘€•ğ‘€­ğ‘€•ğ‘€­ğ‘€•');
      formula = formula.replace('o','ğ‘€•ğ‘€­ğ‘€•ğ‘€•');
      formula = formula.replace('á¹£','ğ‘€•ğ‘€­ğ‘€•ğ‘€•');
      formula = formula.replace('a','ğ‘€­ğ‘€­ğ‘€­');
      formula = formula.replace('m','ğ‘€­ğ‘€­ğ‘€­');
      formula = formula.replace('Ä','ğ‘€•ğ‘€­ğ‘€­');
      formula = formula.replace('t','ğ‘€•ğ‘€­ğ‘€­');
      formula = formula.replace('i','ğ‘€­ğ‘€•ğ‘€­');
      formula = formula.replace('k','ğ‘€­ğ‘€•ğ‘€­');
      formula = formula.replace('e','ğ‘€­ğ‘€•ğ‘€•');
      formula = formula.replace('b','ğ‘€­ğ‘€•ğ‘€•');
      formula = formula.replace('u','ğ‘€­ğ‘€­ğ‘€•');
      formula = formula.replace('l','ğ‘€­ğ‘€­ğ‘€•');
      formula = formula.replace('Ä«','ğ‘€•ğ‘€­ğ‘€•');
      formula = formula.replace('Å›','ğ‘€•ğ‘€­ğ‘€•');
      formula = formula.replace('á¹›','ğ‘€­ğ‘€•ğ‘€•');
      formula = formula.replace('á¹…','ğ‘€­ğ‘€•ğ‘€•');
      formula = formula.replace('Å«','ğ‘€•ğ‘€•ğ‘€•');
      formula = formula.replace('g','ğ‘€•ğ‘€•ğ‘€•');
      formula = formula.replace('á¹…','ğ‘€­ğ‘€•ğ‘€•');
      formula = formula.replace('d','ğ‘€­ğ‘€­ğ‘€­ğ‘€­');
